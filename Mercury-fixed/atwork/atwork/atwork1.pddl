(define (problem atwork1)
        (:domain atwork)
        (:objects o1 o2 o3 - object
		 s1 s2 s3 START END - place
		 youbot - robot
		)
        (:init (On o1 s1 center)
	       (On o2 s2 center)
	       (On o3 s3 center)
	       (Free s1 left)
	       (Free s1 leftcenter)
	       (not(Free s1 center))
	       (Free s1 rightcenter)
	       (Free s1 right)
	       (Free s2 left)
	       (Free s2 leftcenter)
	       (not(Free s2 center))
	       (Free s2 rightcenter)
	       (Free s2 right)
	       (Free s3 left)
	       (Free s3 leftcenter)
	       (not(Free s3 center))
	       (Free s3 rightcenter)
	       (Free s3 right)
	       (Empty youbot)
	       (not(Full youbot))
	       (PlatformFree youbot p1)
	       (PlatformFree youbot p2)
	       (PlatformFree youbot p3)
	       (not(Safe youbot))
	       (At START)
	       (not(At s1))
	       (not(At s2))
	       (not(At s3))
	       (not(At END))
       )
   (:goal 
     (and (On o1 s3 left)
	  (On o2 s3 center)
	  (On o3 s3 right)
	  (At END)
     )
   )
)

